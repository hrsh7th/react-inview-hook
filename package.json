{
  "name": "react-inview-hook",
  "version": "0.0.5",
  "description": "react hooks & intersection-observer",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "build": "rimraf lib/**/*.{js,map} && tsc -p ./tsconfig.esm.json && tsc -p ./tsconfig.cjs.json",
    "test": "jest --config=./jest.config.js",
    "debug": "webpack-dev-server --config ./webpack.config.js",
    "pretty-quick": "pretty-quick",
    "release": "npm run build && np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "hrsh7th",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hrsh7th/react-inview-hook.git"
  },
  "keywords": [
    "react",
    "intersection-observer"
  ],
  "files": [
    "lib"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "husky": "^3.0.4",
    "intersection-observer": "^0.7.0",
    "jest": "^24.9.0",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
